<% content_for :title, "Editing administrator" %>

<% form_for(@administrator) do |f| %>
  <%= f.error_messages %>

  <dl>
    <dt>
      <%= f.label :name %>
    </dt>
    <dd>
      <%= f.text_field :name %>
    </dd>
  </dl>
  
  <%= f.submit 'Update' %>
  
<% end %>

<%= link_to 'Back', administrators_path %>